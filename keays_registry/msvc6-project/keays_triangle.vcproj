				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="..\..\keays_types\include,..\..\keays_math\include"
				PreprocessorDefinitions="WIN32;_DEBUG;_WINDOWS;_USRDLL;KEAYS_TRIANGLE_EXPORTS"
				BasicRuntimeChecks="3"
				RuntimeLibrary="3"
				PrecompiledHeaderFile=".\..\build\Debug/keays_triangle.pch"
				AssemblerListingLocation=".\..\build\Debug/"
				ObjectFile=".\..\build\Debug/"
				ProgramDataBaseFileName=".\..\build\Debug/"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="odbc32.lib odbccp32.lib keays_typesD.lib keays_mathD.lib"
				OutputFile="..\build\Debug/keays_triangleD.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="..\..\keays_types\bin,..\..\keays_math\bin"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\..\build\Debug/keays_triangleD.pdb"
				ImportLibrary=".\..\build\Debug/keays_triangleD.lib"/>
