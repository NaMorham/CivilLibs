				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\build\Debug/keays_triangle.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"
				Description="Copy to bin directory (DEBUG)"
				CommandLine="copy $(OutDir)\keays_triangleD.dll ..\bin
copy $(OutDir)\keays_triangleD.lib ..\bin
"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="3081"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
